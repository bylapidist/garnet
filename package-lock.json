{
  "name": "@lapidist/garnet",
  "version": "0.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "@lapidist/garnet",
      "version": "0.0.0",
      "workspaces": [
        "apps/*",
        "packages/*"
      ],
      "devDependencies": {}
    },
    "apps/storybook": {
      "name": "@lapidist/garnet-storybook",
      "version": "0.0.0",
      "dependencies": {
        "@lapidist/garnet-components": "*"
      },
      "devDependencies": {}
    },
    "apps/website": {
      "name": "@lapidist/garnet-website",
      "version": "0.0.0",
      "dependencies": {
        "@lapidist/garnet-components": "*"
      },
      "devDependencies": {}
    },
    "node_modules/@lapidist/garnet-components": {
      "resolved": "packages/components",
      "link": true
    },
    "node_modules/@lapidist/garnet-scripts": {
      "resolved": "packages/scripts",
      "link": true
    },
    "node_modules/@lapidist/garnet-storybook": {
      "resolved": "apps/storybook",
      "link": true
    },
    "node_modules/@lapidist/garnet-styles": {
      "resolved": "packages/styles",
      "link": true
    },
    "node_modules/@lapidist/garnet-types": {
      "resolved": "packages/types",
      "link": true
    },
    "node_modules/@lapidist/garnet-website": {
      "resolved": "apps/website",
      "link": true
    },
    "packages/components": {
      "name": "@lapidist/garnet-components",
      "version": "0.0.0",
      "dependencies": {
        "@lapidist/garnet-scripts": "*",
        "@lapidist/garnet-styles": "*",
        "@lapidist/garnet-types": "*"
      },
      "devDependencies": {}
    },
    "packages/scripts": {
      "name": "@lapidist/garnet-scripts",
      "version": "0.0.0",
      "devDependencies": {}
    },
    "packages/styles": {
      "name": "@lapidist/garnet-styles",
      "version": "0.0.0",
      "devDependencies": {}
    },
    "packages/types": {
      "name": "@lapidist/garnet-types",
      "version": "0.0.0",
      "devDependencies": {}
    }
  }
}
